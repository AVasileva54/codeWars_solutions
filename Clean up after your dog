//https://www.codewars.com/kata/57faa6ff9610ce181b000028/train/javascript

function crap(x, bags, cap){
  let total = bags * cap;
  sumOfCrap = 0;
  for (let ar of x) {
   for (let el of ar) {
    if (el === 'D') return 'Dog!!';
    if (el === '@') sumOfCrap++;
    }
  }
    return total >= sumOfCrap? 'Clean': 'Cr@p';
}
