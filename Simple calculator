function calculator(a,b,sign){
switch (a){
    case a !== 'number':  return 'unknown value';
    };
switch (b){
      case b !== 'number': return 'unknown value';
      };
switch (sign) {
    case '+': return a+b;
    case '-': return a-b;
    case '*': return a*b;
    case '/': return a/b;
   default: return "unknown value";
  }
}
