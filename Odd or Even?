function oddOrEven(array) {
  console.log(array)
  let sum = 0;
  for(let i = 0; i < array.length; i++){
    sum += array[i];
  }
  return sum % 2 === 0 ? 'even' : 'odd';
}

___________________________________________________________
function oddOrEven(arr) {
  return arr.reduce((a, b) => a + b, 0) % 2 ? 'odd' : 'even';
}
